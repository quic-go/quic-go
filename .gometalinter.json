{
  "DisableAll": true,
  "Exclude": [
    "vendor",
    "streams_map_incoming_generic.go",
    "streams_map_outgoing_generic.go"
  ],
  "Enable": [
    "deadcode",
    "goimports",
    "ineffassign",
    "megacheck",
    "misspell",
    "structcheck",
    "unconvert",
    "varcheck",
    "vet"
  ],
  "Linters": {
    "vet": "go tool vet -printfuncs=Infof,Debugf,Warningf,Errorf:PATH:LINE:MESSAGE"
  }
}
